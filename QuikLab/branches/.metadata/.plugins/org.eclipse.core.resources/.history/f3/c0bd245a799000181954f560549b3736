#! /usr/bin/env python
#coding=GB18030
from pywinauto import application
from pywinauto import mouse as ms
import auto_lib as pywin 
import SendKeys
import time
import os
app=pywin.Pywin()

def login(tl_dir,tl_name,key):
    app.start(tl_dir,tl_name)
#     window_name =u'登录--试验自动测试管理系统'
#     app.connect(window_name)
